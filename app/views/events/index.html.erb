<% @events.each do |event| %>
  <h1><%= event.name %> </h1>
  <h3><%= event.date %> </h3>
  <h3><%= event.time %></h3>
  <h3><%= event.cost %></h3>
  <h3><%= event.location %></h3>
  <h3><%= event.description %></h3>
  <%if event.user_id == session[:user_id] %>
    <%=link_to 'Edit Event', edit_event_path(event) %>
    <%end%>
<%end%>
