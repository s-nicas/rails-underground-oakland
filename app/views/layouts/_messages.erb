
<% if flash[:warning] %>
  <div class="alert alert-warning">
    <%= flash[:warning] %>
  </div>
<% end %>


<% if flash[:success] %>
  <div class="alert alert-success">
    <%= flash[:sucess] %>
  </div>
<% end %>


<% if flash[:info] %>
  <div class="alert alert-info">
      <%= flash[:info] %>
  </div>
<% end %>


<% if flash[:danger] %>
  <div class="alert alert-danger">
    <%= flash[:danger] %>
  </div>
<%end%>
